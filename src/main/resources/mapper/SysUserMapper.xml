        <result column="zhicheng" property="zhicheng" />
        <result column="phone" property="phone" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, password, name, role, sex, age, classess, zhicheng, phone
    </sql>

</mapper>
